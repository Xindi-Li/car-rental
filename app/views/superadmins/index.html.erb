<p id="notice"><%= notice %></p>
<div class="row">
  <div class="col-md-6 col-md-offset-3">

    <h1>Superadmins</h1>

    <table class="table table-striped table-bordered">
      <thead>
      <tr>
        <th>email</th>
        <th>name</th>
        <th colspan="2">operation</th>
      </tr>
      </thead>

      <tbody>
      <% @superadmins.each do |superadmin| %>
          <tr>
            <td><%= superadmin.email %></td>
            <td><%= superadmin.name %></td>
            <td><%= link_to 'Show', superadmin_path(superadmin) %></td>
          </tr>
      <% end %>
      </tbody>
    </table>
    <br>
    <%= link_to 'New Superadmin', new_superadmin_path, class: "btn btn-primary" %>
  </div>
</div>
